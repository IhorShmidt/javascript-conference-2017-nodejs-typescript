{
  "name": "basta-herbst-2017-nodejs-typescript",
  "version": "1.0.0",
  "description": "code sample for the talk \"Web-APIs mit Node.js und TypeScript – für .NET-Entwickler\" at BASTA! conference 2017 in Mainz.",
  "keywords": [
    "typeorm",
    "webapi",
    "api",
    "web",
    "web api",
    "typescript"
  ],
  "scripts": {
    "docker-build": "docker build -t confmr.azurecr.io/bastaherbst2017nodejswebapi:latest .",
    "docker-run": "docker run confmr.azurecr.io/bastaherbst2017nodejswebapi:latest",
    "docker-push": "docker push confmr.azurecr.io/bastaherbst2017nodejswebapi",
    "build": "rimraf build && tsc",
    "start": "nodemon --exec ts-node src/index.ts",
    "start-server": "node build/index.js"
  },
  "author": "Manuel Rauber <manuel.rauber@thinktecture.com>",
  "license": "ISC",
  "dependencies": {
    "@types/restify": "^5.0.3",
    "nodemon": "^1.12.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "restify": "^6.0.0",
    "rimraf": "^2.4.5",
    "ts-node": "^3.3.0",
    "typeorm": "0.1.0-alpha.42",
    "typescript": "^2.5.2"
  }
}
